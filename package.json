{
  "name": "@ty91/browser-cli",
  "version": "0.1.0",
  "description": "CLI browser control with daemon/IPC session core",
  "type": "module",
  "bin": {
    "browser": "dist/bin/cdt.js",
    "browser-daemon": "dist/bin/cdt-daemon.js"
  },
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "dev": "tsx src/bin/cdt.ts",
    "daemon": "tsx src/bin/cdt-daemon.ts",
    "lint": "eslint .",
    "prepublishOnly": "pnpm build",
    "typecheck": "tsc -p tsconfig.typecheck.json --noEmit",
    "test": "vitest run"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "cdp",
    "cli",
    "daemon"
  ],
  "author": "",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "packageManager": "pnpm@10.27.0",
  "dependencies": {
    "chrome-launcher": "^1.2.1",
    "commander": "^14.0.3",
    "playwright": "^1.58.2",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@eslint/js": "^10.0.1",
    "@types/node": "^25.3.0",
    "eslint": "^10.0.1",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.56.0",
    "vitest": "^4.0.18"
  }
}
